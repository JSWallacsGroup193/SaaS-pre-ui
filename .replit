entrypoint = "index.js"
modules = ["python-3.11", "postgresql-16", "nodejs-20"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_11"
packages = ["firebird_3"]

[deployment]
run = ["sh", "-c", "cd backend && npm run start:prod"]
deploymentTarget = "autoscale"
ignorePorts = false
build = ["bash", "build.sh"]

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "HVAC App"

[[workflows.workflow]]
name = "HVAC App"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

